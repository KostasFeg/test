{
  "name": "Enterprise Blue Theme",
  "description": "Professional corporate theme with comprehensive financial analytics and enterprise-grade reporting capabilities. Perfect for executive dashboards and financial oversight.",
  "config": {
    "branding": {
      "company": {
        "name": "Enterprise Analytics Corp",
        "displayName": "Enterprise Analytics Corporation"
      },
      "app": {
        "name": "Executive Dashboard",
        "version": "2.1.0",
        "description": "Comprehensive enterprise analytics and reporting platform"
      }
    },
    "theme": {
      "colors": {
        "primary": "#1e40af",
        "secondary": "#3b82f6",
        "accent": "#06b6d4",
        "success": "#059669",
        "warning": "#d97706",
        "error": "#dc2626",
        "info": "#0284c7"
      },
      "shadows": {
        "shadowSmall": "0 1px 3px rgba(0, 0, 0, 0.06)",
        "shadowMedium": "0 4px 12px rgba(0, 0, 0, 0.08)",
        "shadowLarge": "0 12px 24px rgba(0, 0, 0, 0.12)",
        "shadowXLarge": "0 24px 48px rgba(0, 0, 0, 0.18)",
        "shadowInner": "inset 0 2px 4px rgba(0, 0, 0, 0.06)",
        "shadowFocus": "0 0 0 3px rgba(30, 64, 175, 0.15)"
      },
      "transitions": {
        "transitionFast": "0.12s",
        "transitionMedium": "0.25s",
        "transitionSlow": "0.4s",
        "easingDefault": "cubic-bezier(0.4, 0, 0.2, 1)",
        "easingBounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
        "easingSharp": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "borderRadius": {
        "radiusSmall": "0.375rem",
        "radiusMedium": "0.5rem",
        "radiusLarge": "0.75rem",
        "radiusXLarge": "1rem"
      }
    },
    "layout": {
      "sidebarWidth": {
        "buttons": "90px",
        "labels": "320px"
      },
      "topBarHeight": "72px",
      "bottomBarHeight": "0px"
    },
    "ui": {
      "defaultSidebarVariant": "labels",
      "defaultShowBottomBar": false,
      "animationFast": "0.12s",
      "animationMedium": "0.25s",
      "animationSlow": "0.4s"
    },
    "features": {
      "enableDebugReports": true,
      "enableAdvancedFilters": true,
      "enableThemeCustomization": true,
      "enableBulkOperations": true,
      "enableOfflineMode": false
    },
    "navigation": [
      {
        "slug": "executive-dashboard",
        "label": "üìä Executive Dashboard"
      },
      {
