    "reports": {
      "revenue-analysis": {
        "name": "Revenue Analysis Report",
        "filters": ["type", "scope", "fromDate", "toDate"],
        "options": {
          "type": ["Daily", "Weekly", "Monthly", "Quarterly", "Annual"],
          "scope": ["Division", "Department", "Product Line", "Geographic Region"],
          "withTime": false,
          "withAutoTime": true
        }
      },
      "profit-margins": {
        "name": "Profit Margin Analysis",
        "filters": ["type", "scope", "fromDate", "toDate"],
        "options": {
