{
  "name": "Retail & E-Commerce Theme",
  "description": "Dynamic retail-focused theme with commerce analytics, inventory management, and customer insights. Perfect for retail chains, e-commerce platforms, and omnichannel operations.",
  "config": {
    "branding": {
      "company": {
        "name": "RetailMax Analytics",
        "displayName": "RetailMax Commerce Intelligence Platform"
      },
      "app": {
        "name": "Omnichannel Analytics Suite",
        "version": "5.1.3",
        "description": "Complete retail and e-commerce analytics platform for modern retailers"
      }
    },
    "theme": {
      "colors": {
        "primary": "#ea580c",
        "secondary": "#dc2626",
        "accent": "#f59e0b",
        "success": "#059669",
        "warning": "#d97706",
        "error": "#dc2626",
        "info": "#0284c7"
      },
      "shadows": {
        "shadowSmall": "0 1px 3px rgba(234, 88, 12, 0.1)",
        "shadowMedium": "0 4px 12px rgba(234, 88, 12, 0.15)",
        "shadowLarge": "0 12px 24px rgba(234, 88, 12, 0.2)",
        "shadowXLarge": "0 24px 48px rgba(234, 88, 12, 0.25)",
        "shadowInner": "inset 0 2px 4px rgba(234, 88, 12, 0.1)",
        "shadowFocus": "0 0 0 3px rgba(234, 88, 12, 0.2)"
      },
      "transitions": {
        "transitionFast": "0.1s",
        "transitionMedium": "0.2s",
        "transitionSlow": "0.35s",
        "easingDefault": "cubic-bezier(0.4, 0, 0.2, 1)",
        "easingBounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
        "easingSharp": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "borderRadius": {
        "radiusSmall": "0.375rem",
        "radiusMedium": "0.5rem",
        "radiusLarge": "0.75rem",
        "radiusXLarge": "1rem"
      }
    },
    "layout": {
      "sidebarWidth": {
        "buttons": "88px",
        "labels": "300px"
      },
      "topBarHeight": "66px",
      "bottomBarHeight": "75px"
    },
    "ui": {
      "defaultSidebarVariant": "labels",
      "defaultShowBottomBar": true,
      "animationFast": "0.1s",
      "animationMedium": "0.2s",
      "animationSlow": "0.35s"
    },
    "features": {
      "enableDebugReports": true,
      "enableAdvancedFilters": true,
      "enableThemeCustomization": true,
      "enableBulkOperations": true,
      "enableOfflineMode": true
