    "reports": {
      "patient-demographics": {
        "name": "Patient Demographics Analysis",
        "filters": ["type", "scope", "fromDate", "toDate"],
        "options": {
          "type": ["Age Groups", "Gender Distribution", "Insurance Types", "Geographic Distribution"],
          "scope": ["Emergency Department", "Inpatient", "Outpatient", "ICU"],
          "withTime": false,
          "withAutoTime": true
        }
      },
      "patient-outcomes": {
        "name": "Patient Outcomes Report",
        "filters": ["type", "scope", "fromDate", "toDate"],
        "options": {
          "type": ["Clinical Outcomes", "Functional Outcomes", "Patient-Reported Outcomes"],
          "scope": ["By Department", "By Diagnosis", "By Provider", "By Treatment Protocol"],
          "withTime": false,
          "withAutoTime": true
        }
      },
      "readmission-rates": {
