{
  "name": "Healthcare Professional Theme",
  "description": "Calming, professional healthcare theme with medical-grade analytics, patient management reports, and comprehensive compliance tracking. Perfect for healthcare organizations.",
  "config": {
    "branding": {
      "company": {
        "name": "MedTech Analytics",
        "displayName": "MedTech Healthcare Analytics Platform"
      },
      "app": {
        "name": "Healthcare Intelligence Suite",
        "version": "4.2.1",
        "description": "Comprehensive healthcare analytics and patient management platform"
      }
    },
    "theme": {
      "colors": {
        "primary": "#059669",
        "secondary": "#0d9488",
        "accent": "#06b6d4",
        "success": "#10b981",
        "warning": "#f59e0b",
        "error": "#dc2626",
        "info": "#3b82f6"
      },
      "shadows": {
        "shadowSmall": "0 1px 3px rgba(5, 150, 105, 0.08)",
        "shadowMedium": "0 4px 12px rgba(5, 150, 105, 0.1)",
        "shadowLarge": "0 12px 24px rgba(5, 150, 105, 0.12)",
        "shadowXLarge": "0 24px 48px rgba(5, 150, 105, 0.15)",
        "shadowInner": "inset 0 2px 4px rgba(5, 150, 105, 0.06)",
        "shadowFocus": "0 0 0 3px rgba(5, 150, 105, 0.15)"
      },
      "transitions": {
        "transitionFast": "0.2s",
        "transitionMedium": "0.35s",
        "transitionSlow": "0.5s",
        "easingDefault": "cubic-bezier(0.4, 0, 0.2, 1)",
        "easingBounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
        "easingSharp": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "borderRadius": {
        "radiusSmall": "0.25rem",
        "radiusMedium": "0.375rem",
        "radiusLarge": "0.5rem",
        "radiusXLarge": "0.75rem"
      }
    },
    "layout": {
      "sidebarWidth": {
        "buttons": "95px",
        "labels": "350px"
      },
      "topBarHeight": "68px",
      "bottomBarHeight": "0px"
    },
    "ui": {
      "defaultSidebarVariant": "labels",
      "defaultShowBottomBar": false,
      "animationFast": "0.2s",
      "animationMedium": "0.35s",
      "animationSlow": "0.5s"
    },
    "features": {
      "enableDebugReports": false,
      "enableAdvancedFilters": true,
      "enableThemeCustomization": false,
      "enableBulkOperations": true,
      "enableOfflineMode": false
