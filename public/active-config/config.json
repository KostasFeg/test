{
  "name": "Government & Civic Theme",
  "description": "Professional government and public sector theme with civic analytics, public service metrics, and comprehensive transparency reporting. Perfect for government agencies and civic organizations.",
  "config": {
    "branding": {
      "company": {
        "name": "Civic Analytics Platform",
        "displayName": "Public Sector Intelligence & Transparency Portal"
      },
      "app": {
        "name": "Government Performance Dashboard",
        "version": "6.0.1",
        "description": "Comprehensive public sector analytics and citizen service management platform"
      }
    },
    "theme": {
      "colors": {
        "primary": "#1e3a8a",
        "secondary": "#1f2937",
        "accent": "#dc2626",
        "success": "#059669",
        "warning": "#d97706",
        "error": "#dc2626",
        "info": "#0369a1"
      },
      "shadows": {
        "shadowSmall": "0 1px 3px rgba(30, 58, 138, 0.08)",
        "shadowMedium": "0 4px 12px rgba(30, 58, 138, 0.1)",
        "shadowLarge": "0 12px 24px rgba(30, 58, 138, 0.12)",
        "shadowXLarge": "0 24px 48px rgba(30, 58, 138, 0.15)",
        "shadowInner": "inset 0 2px 4px rgba(30, 58, 138, 0.06)",
        "shadowFocus": "0 0 0 3px rgba(30, 58, 138, 0.15)"
      },
      "transitions": {
        "transitionFast": "0.25s",
        "transitionMedium": "0.4s",
        "transitionSlow": "0.6s",
        "easingDefault": "cubic-bezier(0.4, 0, 0.2, 1)",
        "easingBounce": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
        "easingSharp": "cubic-bezier(0.25, 0.46, 0.45, 0.94)"
      },
      "borderRadius": {
        "radiusSmall": "0.25rem",
        "radiusMedium": "0.375rem",
        "radiusLarge": "0.5rem",
        "radiusXLarge": "0.75rem"
      }
    },
    "layout": {
      "sidebarWidth": {
        "buttons": "100px",
        "labels": "380px"
      },
      "topBarHeight": "76px",
      "bottomBarHeight": "0px"
    },
    "ui": {
      "defaultSidebarVariant": "labels",
      "defaultShowBottomBar": false,
      "animationFast": "0.25s",
      "animationMedium": "0.4s",
      "animationSlow": "0.6s"
    },
    "features": {
      "enableDebugReports": false,
      "enableAdvancedFilters": true,
      "enableThemeCustomization": false,
      "enableBulkOperations": true,
      "enableOfflineMode": false
    },
