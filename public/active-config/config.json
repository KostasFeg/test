    "reports": {
      "service-requests": {
        "name": "Citizen Service Requests Analysis",
        "filters": ["type", "scope", "fromDate", "toDate"],
        "options": {
          "type": ["Total Requests", "Request Categories", "Resolution Status", "Priority Levels"],
          "scope": ["All Departments", "Public Works", "Police", "Fire", "Parks & Recreation"],
          "withTime": false,
          "withAutoTime": true
        }
      },
      "permit-applications": {
        "name": "Permit & License Applications",
        "filters": ["type", "scope", "fromDate", "toDate"],
        "options": {
          "type": ["Building Permits", "Business Licenses", "Special Events", "Zoning Requests"],
          "scope": ["New Applications", "Pending Review", "Approved", "Rejected"],
          "withTime": false,
          "withAutoTime": true
